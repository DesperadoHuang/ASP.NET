<configuration>

    <system.web>
      <compilation debug="true" targetFramework="4.5.2" />
      <httpRuntime targetFramework="4.5.2" />
    </system.web>
<!--4.5以上的版本需加入以下設定 詳細請參考http://zackwang.blogspot.tw/2015/02/webforms-unobtrusivevalidationmode.html-->
  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None" />
  </appSettings>
</configuration>
